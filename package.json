{
  "name": "md2json",
  "version": "1.0.0",
  "description": "Parse Markdown files to JSON",
  "main": "./dist/md2json.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npm run clean && $(npm bin)/webpack --env.prod --optimize-dedupe",
    "postbuild": "$(npm bin)/uglifyjs ./dist/md2json.js -o ./dist/md2json.js --source-map ./dist/md2json.js.map --source-map-root ./src/main.js --in-source-map ./dist/md2json.js.map --screw-ie8 -c -m",
    "watch": "npm run clean && $(npm bin)/webpack --watch --verbose",
    "prepublish": "npm run build",
    "test": "node ./test/script.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/f0rr0/md2json.git"
  },
  "keywords": [
    "markdown",
    "json",
    "yaml",
    "frontmatter"
  ],
  "author": "Sid Jain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/f0rr0/md2json/issues"
  },
  "homepage": "https://github.com/f0rr0/md2json#readme",
  "dependencies": {
    "marked": "^0.3.6",
    "yaml-front-matter": "^3.4.0"
  },
  "devDependencies": {
    "uglify-js": "git://github.com/mishoo/UglifyJS2.git#harmony",
    "webpack": "^2.1.0-beta.22"
  },
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.8.6"
  }
}
